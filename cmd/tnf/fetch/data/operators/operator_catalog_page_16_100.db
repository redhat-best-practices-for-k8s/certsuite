      "alm_examples": [
        {
          "api_version": "optimize.stormforge.io/v1beta2",
          "kind": "Experiment",
          "metadata": {
            "labels": {
              "stormforge.io/application": "my-app",
              "stormforge.io/scenario": "black-friday"
            },
            "name": "experiment-sample"
          },
          "spec": {
            "metrics": [
              {
                "minimize": true,
                "name": "duration",
                "query": "{{ duration .StartTime .CompletionTime }}"
              },
              {
                "minimize": true,
                "name": "resource_requests",
                "query": "{{ resourceRequests .Target \"cpu=0.017,memory=0.000000000003\" }}",
                "target": {
                  "apiVersion": "v1",
                  "kind": "PodList"
                }
              }
            ],
            "parameters": [
              {
                "baseline": 1000,
                "max": 2000,
                "min": 500,
                "name": "cpu"
              },
              {
                "baseline": 2048,
                "max": 4096,
                "min": 1024,
                "name": "memory"
              }
            ],
            "patches": [
              {
                "patch": "spec:\n  template:\n    spec:\n      containers:\n      - name: postgres\n        resources:\n          limits:\n            cpu: '{{ .Values.cpu }}m'\n            memory: '{{ .Values.memory }}Mi'\n          requests:\n            cpu: '{{ .Values.cpu }}m'\n            memory: '{{ .Values.memory }}Mi'\n",
                "targetRef": {
                  "apiVersion": "apps/v1",
                  "kind": "Deployment",
                  "name": "my-app"
                }
              }
            ],
            "trialTemplate": {
              "spec": {
                "initialDelaySeconds": 5,
                "jobTemplate": {
                  "spec": {
                    "template": {
                      "spec": {
                        "containers": [
                          {
                            "args": [
                              "--testCase=black-friday"
                            ],
                            "env": [
                              {
                                "name": "LOAD_TEST_URL",
                                "value": "http://my-app/"
                              }
                            ],
                            "image": "my-load-test:latest",
                            "name": "black-friday"
                          }
                        ]
                      }
                    }
                  }
                }
              }
            }
          }
        }
      ],
